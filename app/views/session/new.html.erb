<h1>Login Page</h1>

<% if flash[:error].present?%>
    <%= flash[:error]  %>
<% end %>

<%= form_with url: login_path do |f|%> <!-- login path was url previously-->

<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>


<%= f.submit "Login" %>

<% end %>





